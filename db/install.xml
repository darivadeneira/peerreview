<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/workshop/eval/peerreview/db" VERSION="20251901" COMMENT="XMLDB file for Moodle mod/workshop/eval/peerreview"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="workshopeval_peerreview" COMMENT="Settings for the grading evaluation subplugin Comparison the assessment with IA.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="assessmentid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="feedback_ai" TYPE="CHAR" LENGTH="25" NOTNULL="false" DEFAULT="Pendiente" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="fkuq_workshop" TYPE="foreign-unique" FIELDS="assessmentid" REFTABLE="workshop_assessment" REFFIELDS="id" COMMENT="Every workshop can have only one settings record"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
